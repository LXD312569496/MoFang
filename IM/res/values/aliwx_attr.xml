    <declare-styleable name="ChattingSelfHelpMenuBar">
        <attr name="menuTextSize" format="reference|integer"/>
        <attr name="subMenuTextSize" format="reference|integer"/>
        <attr name="menuTextColor" format="reference"/>
        <attr name="subMenuTextColor" format="reference"/>
        <attr name="subMenuMaxTextLength" format="reference|integer"/>
        <attr name="menuMaxTextLength" format="reference|integer"/>
    </declare-styleable>
    <declare-styleable name="aliwx_PullToRefresh">
        <attr name="aliwx_ptrShowIndicator" format="reference|boolean" />
    </declare-styleable>

    <declare-styleable name="ShaderImageView">
        <attr name="siArrowPosition"/>
    </declare-styleable>

    <attr name="siArrowPosition">
        <enum name="left" value="0" />
        <enum name="right" value="1" />
    </attr>

    <declare-styleable name="SelfHelpMenuItem">
        <attr name="menuItemTextColor" format="reference|color"/>
        <attr name="menuItemTextSize" format="dimension|integer"/>
    </declare-styleable>

    <declare-styleable name="PriceTextView">
        <attr name="signSize" format="dimension"/>
        <attr name="integerSize" format="dimension"/>
        <attr name="floatSize" format="dimension"/>
        <attr name="needDeleteLine" format="boolean"/>
        <attr name="showPostfixZero" format="boolean"/>
        <attr name="needTruncate" format="boolean"/>
    </declare-styleable>

